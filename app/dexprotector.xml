<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dexprotector>
    <verbose>false</verbose>
    <optimize>false</optimize>
    <signMode>release</signMode>	
    <detect>
      <!-- for debugging purposes only -->  
      <trace>9</trace>
      <integrity>
        <positiveCheckCallback>com.dexprotector.detector.integrity.MainActivity.positiveTamperCheck</positiveCheckCallback>
        <negativeCheckCallback>com.dexprotector.detector.integrity.MainActivity.negativeTamperCheck</negativeCheckCallback>
        <probeMethod>com.dexprotector.detector.integrity.MainActivity.doProbe</probeMethod>
       </integrity>
    </detect>
    <integrityControl>
        <checkCertificate>off</checkCertificate>
    </integrityControl>
</dexprotector>

